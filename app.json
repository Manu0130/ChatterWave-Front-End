{
  "expo": {
    "name": "ChatterWave",
    "slug": "ChatterWave",
    "scheme": "chatterwave",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/chatIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/chatIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/chatIcon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.manujaya_30.ChatterWave"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-router"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3f78f90d-89fa-4124-8c34-782c52311a4d"
      }
    }
  }
}
